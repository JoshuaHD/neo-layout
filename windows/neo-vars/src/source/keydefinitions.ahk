; RegisterAndHookSC
RSC(sc,vk) {
  global
  vksc%sc% := "VK" . vk . "SC" . sc
  dnkey := "*" . vksc%sc%
  upkey := dnkey . " up"
  Hotkey,% dnkey,allstarhook
  Hotkey,% upkey,allstarhook
}

